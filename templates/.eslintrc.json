{
	"env": {
		"browser": true,
		"es2021": true
	},
	"plugins": ["svelte3"],
	"overrides": [
		{
			"files": ["*.svelte"],
			"processor": "svelte3/svelte3"
		}
	],
	"extends": ["airbnb-base", "prettier"],
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"settings": {
		"import/resolver": {
			"alias": {
				"map": [["\\$lib", "./src/lib"]]
			}
		}
	},
	"rules": {}
}
